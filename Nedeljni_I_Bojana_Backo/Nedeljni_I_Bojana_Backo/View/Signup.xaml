<Window x:Class="Nedeljni_I_Bojana_Backo.View.Signup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nedeljni_I_Bojana_Backo.View"
        mc:Ignorable="d"
        Title="" Height="250" Width="200" FontSize="14" Background="AntiqueWhite" WindowStartupLocation="CenterScreen">
    <Border Background="AntiqueWhite" CornerRadius="20" Margin="20">
        <StackPanel Margin="20">
            <Button Name="btnDoctor" Command="{Binding SignupManager}" Content="Manager" Margin="10 10"/>
            <Button Name="btnSignup" Command="{Binding SignupEmployee}" Content="Employee" Margin="10 10"/>
            <Button Name="btnClose" Command="{Binding Cancel}" Content="Close" Margin="10 10"/>
        </StackPanel>
    </Border>
</Window>
